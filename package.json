{
  "name": "brewery-management-platform",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "prune:backend": "turbo prune --scope='backend' --docker",
    "dev": "turbo run start:dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "^1.9.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@9.6.2"
}
